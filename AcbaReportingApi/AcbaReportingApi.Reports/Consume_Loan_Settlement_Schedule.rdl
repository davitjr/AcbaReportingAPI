    <DataSet Name="Loan_Grafik_Property">
      <Query>
        <DataSourceName>AccOperbaseDataSource</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@customerNumber">
            <Value>=Parameters!customerNumber.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Language_id">
            <Value>=Parameters!Language_id.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
        </QueryParameters>
        <CommandText>
          DECLARE @ourBank AS nvarchar(100)
          SET                @ourBank = CASE WHEN @Language_id = 0 THEN
          (SELECT        dbo.fnc_convertAnsiToUnicode(Description)
          FROM            [Tbl_banks;]
          WHERE        code = 22000) ELSE
          (SELECT        Description_Engl
          FROM            [Tbl_banks;]
          WHERE        code = 22000) END
          SELECT        @ourBank ourBank, CASE WHEN type_of_client = 6 THEN CASE WHEN @Language_id = 0 THEN dbo.fnc_convertAnsiToUnicode(lastName) + ' ' + dbo.fnc_convertAnsiToUnicode(name)
          ELSE lastNAmeEng + ' ' + nameEng END ELSE CASE WHEN @Language_id = 0 THEN dbo.fnc_convertAnsiToUnicode(description) ELSE description_Eng END END customerName
          FROM            V_CustomerDesription
          WHERE        customer_number = @customerNumber
        </CommandText>
      </Query>
      <Fields>
        <Field Name="ourBank">
          <DataField>ourBank</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="customerName">
          <DataField>customerName</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="product_type">
          <DataField>product_type</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="loan_type">
          <DataField>loan_type</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="kind_of_money">
          <DataField>kind_of_money</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
      </Fields>
    </DataSet>
