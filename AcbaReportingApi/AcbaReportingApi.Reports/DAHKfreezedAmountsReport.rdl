  <DataSets>
    <DataSet Name="DAHKfreezeAmounts">
      <Query>
        <DataSourceName>AccOperBase</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@date">
            <Value>=Parameters!date.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@filialcode">
            <Value>=Parameters!filialcode.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT inquestid, messageid 
INTO #tmp1
FROM DAHK_Base.[dbo].[Tbl_CheckMsg_Response_Attach]
WHERE Request_Date=@date

SELECT customer_number, d.inquestid, d.message_ID, f.account_number, UnUsed_amount, o.overdraft_account
INTO #tmp2
FROM tbl_dahk_freeze_details d
INNER JOIN tbl_acc_freeze_history f ON d.freeze_id=f.Id
INNER JOIN #tmp1 t ON t.inquestid = d.inquestid AND t.messageid=d.message_ID
OUTER APPLY (
		SELECT overdraft_account FROM tbl_visa_numbers_accounts
		WHERE visa_number=d.card_number) o

CREATE NONCLUSTERED INDEX [IX_customer_number_tmp2] ON #tmp2 (customer_number ASC)

SELECT t.customer_number, inquestid, [dbo].[fnc_convertAnsiToUnicode](message_ID) message_ID, account_number, UnUsed_amount, overdraft_account
FROM #tmp2 t
INNER JOIN Tbl_customers c ON c.customer_number=t.customer_number
WHERE c.filialcode=@filialcode</CommandText>
      </Query>
      <Fields>
        <Field Name="customer_number">
          <DataField>customer_number</DataField>
          <rd:TypeName>System.Double</rd:TypeName>
        </Field>
        <Field Name="inquestid">
          <DataField>inquestid</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="message_ID">
          <DataField>message_ID</DataField>
