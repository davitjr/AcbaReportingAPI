    <DataSet Name="Loan_Grafik_Property">
      <Query>
        <DataSourceName>AccOperbaseDataSource</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@loanFullNumber">
            <Value>=Parameters!loanFullNumber.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
          <QueryParameter Name="@dateOfBeginning">
            <Value>=Parameters!dateOfBeginning.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
          <QueryParameter Name="@Language_id">
            <Value>=Parameters!Language_id.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
        </QueryParameters>
        <CommandText>DECLARE @ourBank AS nvarchar(100)

SET @ourBank=CASE WHEN @Language_id=0 THEN 
(SELECT dbo.fnc_convertAnsiToUnicode (Description)  FROM [Tbl_banks;] 
	WHERE code =22000) ELSE
(SELECT Description_Engl  FROM [Tbl_banks;] WHERE code =22000) END	  		

SELECT  
@ourBank ourBank, 
CASE WHEN 
            c.type_of_client=6  THEN
	CASE WHEN @Language_id=0 THEN 
	  dbo.fnc_convertAnsiToUnicode(c.lastName)+' '+ dbo.fnc_convertAnsiToUnicode(c.name) 	ELSE c.lastNAmeEng+' '+c.nameEng END ELSE
	CASE  WHEN @Language_id=0 THEN 
	   dbo.fnc_convertAnsiToUnicode(c.description) ELSE c.description_Eng  END  
END customerName ,
product_type,loan_type,kind_of_money,
ISNULL(Subsidia_interest_rate,0) Subsidia_interest_rate,
v.loan_program
FROM V_ShortLoans v
LEFT JOIN V_CustomerDesription c on v.customer_number=c.customer_number
WHERE v.loan_full_number=@loanFullNumber AND v.date_of_beginning=@dateOfBeginning			</CommandText>
      </Query>
      <Fields>
        <Field Name="ourBank">
          <DataField>ourBank</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="customerName">
          <DataField>customerName</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="product_type">
          <DataField>product_type</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="loan_type">
          <DataField>loan_type</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="kind_of_money">
          <DataField>kind_of_money</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="Subsidia_interest_rate">
          <DataField>Subsidia_interest_rate</DataField>
          <rd:TypeName>System.Double</rd:TypeName>
        </Field>
        <Field Name="loan_program">
          <DataField>loan_program</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
