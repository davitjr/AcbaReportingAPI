<?xml version="1.0" encoding="utf-8"?><Report xmlns="http://schemas.microsoft.com/sqlserver/reporting/2010/01/reportdefinition" xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner"><AutoRefresh>0</AutoRefresh><DataSources><DataSource Name="SMSManagement"><DataSourceReference>SMSManagement</DataSourceReference><rd:SecurityType>None</rd:SecurityType><rd:DataSourceID>81c3f493-e052-4921-878e-c576de3460d8</rd:DataSourceID></DataSource></DataSources><DataSets><DataSet Name="SentSMSMessages"><Query><DataSourceName>SMSManagement</DataSourceName><QueryParameters><QueryParameter Name="@identityId"><Value>=Parameters!identityId.Value</Value></QueryParameter></QueryParameters><CommandText>DECLARE @sql as nvarchar(4000)

set @sql = 'select ms.phone_number, CASE WHEN ms.message_type_id=42 THEN STUFF(ms.message_text, 33, 37, ''******'') ELSE ms.message_text END message_text, dbo.FormatDate(ms.registration_date, ''dd/mm/yy hh:nn'') as registration_date, s.[description] as [status], t.[description] as type_description, ms.*
					from tbl_sent_messages ms
							inner join tbl_types_of_message_status s on ms.message_status_id = s.id 
							inner join tbl_types_of_message t on ms.message_type_id = t.id
					where ms.message_type_id&lt;&gt;35 and ms.identity_ID =  ' + @identityId + '  order by ms.registration_date desc '

exec(@sql)</CommandText></Query><Fields><Field Name="registration_date"><DataField>registration_date</DataField><rd:UserDefined>true</rd:UserDefined></Field><Field Name="phone_number"><DataField>phone_number</DataField><rd:UserDefined>true</rd:UserDefined></Field><Field Name="message_text"><DataField>message_text</DataField><rd:UserDefined>true</rd:UserDefined></Field><Field Name="status"><DataField>status</DataField><rd:UserDefined>true</rd:UserDefined></Field><Field Name="type_description"><DataField>type_description</DataField><rd:UserDefined>true</rd:UserDefined></Field></Fields></DataSet></DataSets><ReportSections><ReportSection><Body><ReportItems><Textbox Name="Textbox1"><CanGrow>true</CanGrow><KeepTogether>true</KeepTogether><Paragraphs><Paragraph><TextRuns><TextRun><Value>Հաճախորդին ուղարկված հաղորդագրություններ</Value><Style><FontFamily>Segoe UI</FontFamily><FontSize>12pt</FontSize><FontWeight>Bold</FontWeight><Color>Indigo</Color></Style></TextRun></TextRuns><Style /></Paragraph></Paragraphs><rd:DefaultName>Textbox1</rd:DefaultName><Top>0.0635cm</Top><Left>6.72253cm</Left><Height>0.67937cm</Height><Width>11.34703cm</Width><Style><Border><Style>None</Style></Border><PaddingLeft>2pt</PaddingLeft><PaddingRight>2pt</PaddingRight><PaddingTop>2pt</PaddingTop><PaddingBottom>2pt</PaddingBottom></Style></Textbox><Tablix Name="Tablix1"><TablixBody><TablixColumns><TablixColumn><Width>2.88805cm</Width></TablixColumn><TablixColumn><Width>3.54951cm</Width></TablixColumn><TablixColumn><Width>10.00662cm</Width></TablixColumn><TablixColumn><Width>3.73062cm</Width></TablixColumn><TablixColumn><Width>4.66958cm</Width></TablixColumn></TablixColumns><TablixRows><TablixRow><Height>0.66615cm</Height><TablixCells><TablixCell><CellContents><Textbox Name="Textbox14"><CanGrow>true</CanGrow><KeepTogether>true</KeepTogether><Paragraphs><Paragraph><TextRuns><TextRun><Value>Ա/Թ</Value><Style><FontFamily>Segoe UI</FontFamily><FontWeight>Bold</FontWeight></Style></TextRun></TextRuns><Styl